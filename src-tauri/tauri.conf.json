{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "SlideSounds",
  "version": "0.1.0",
  "identifier": "com.slidesounds.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "SlideSounds",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "resources": ["../public/audio"],
    "iOS": {
      "minimumSystemVersion": "13.0"
    }
  },
  "plugins": {}
}
